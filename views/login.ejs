<!DOCTYPE html>
<head>
  <%- include('header'); %>
  <title>ToDone | Login</title>
  <script type="text/javascript" src="javascripts/login.js"></script>
</head>
<body>
  <body class="container">
    <!-- <h1>Hello World <i class="fa fa-smile-o" aria-hidden="true"></i></h1>-->
    <div class="container-fluid">
      <div class="row">
        <h1 class="text-center">LOGIN</h1>
      </div>
      <div class="row">
        <form class="col-xs-12 col-md-4 col-md-offset-4">
          <div class="form-group">
            <label for="inputUsername" class="grey-text">USERNAME</label>
            <input type="text" class="form-control" id="inputUsername" placeholder="Username" required>
          </div>
          <div class="form-group">
            <label for="inputPassword" class="grey-text">PASSWORD</label>
            <input type="password" class="form-control" id="inputPassword" placeholder="Password" required>
          </div>
          <button type="submit" class="btn button-default col-xs-12" function><b>GO</b></button>
        </form>
      </div>
    </div>
  </body>
  <%- include('footer'); %>
</body>
